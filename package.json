{
  "name": "react-admin-vite-antd5",
  "version": "0.0.0",
  "author": {
    "name": "frontend-winter",
    "url": "https://github.com/frontend-winter/react-admin-vite-antd5"
  },
  "scripts": {
    "dev": "vite --mode development",
    "build": "npm run typecheck && vite build --mode production",
    "serve": "npm run build && vite preview --port 3000",
    "clean": "rm -rf node_modules && rm -rf pnpm-lock.yaml && rm -rf react-admin-vite-antd5",
    "lint": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.00",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@ant-design/pro-components": "^2.3.52",
    "@ant-design/pro-layout": "^7.6.1",
    "@babel/core": "7",
    "@emotion/css": "^11.10.5",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/redux-logger": "^3.0.9",
    "antd": "^5.0.1",
    "dayjs": "^1.11.6",
    "lodash": "^4.17.21",
    "rc-field-form": "1.22",
    "rc-texty": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.0",
    "sass": "^1.56.1"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-community/eslint-plugin": "^1.3.0",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.190",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^2.2.0",
    "crypto-js": "^4.1.1",
    "eslint": "8.22.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.7.1",
    "redux-devtools-extension": "^2.13.9",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.7.4",
    "vite": "^3.2.5",
    "vite-plugin-compression": "^0.5.1"
  }
}
